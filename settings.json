{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "actions": [
    {
      "command": "duplicateTab",
      "keys": "ctrl+shift+d"
    },
    {
      "command": {
        "action": "copy",
        "singleLine": false
      },
      "keys": "ctrl+c"
    },
    {
      "command": {
        "action": "prevTab"
      },
      "keys": "ctrl+shift+tab"
    },
    {
      "command": "paste",
      "keys": "ctrl+v"
    },
    {
      "command": "closePane",
      "keys": "ctrl+shift+w"
    },
    {
      "command": "find",
      "keys": "ctrl+shift+f"
    },
    {
      "command": {
        "action": "nextTab"
      },
      "keys": "ctrl+tab"
    },
    {
      "command": {
        "action": "newTab"
      },
      "keys": "ctrl+t"
    },
    {
      "command": "tabSearch",
      "keys": "ctrl+p"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "auto",
        "splitMode": "duplicate"
      },
      "keys": "alt+shift+d"
    },
    {
      "command": {
        "action": "newWindow"
      },
      "keys": "ctrl+shift+n"
    }
  ],
  "copyFormatting": "none",
  "copyOnSelect": true,
  "defaultProfile": "{2ece5bfe-50ed-5f3a-ab87-5cd4baafed2b}",
  "initialCols": 90,
  "initialRows": 20,
  "profiles": {
    "defaults": {
      "colorScheme": "shades-of-purple",
      "font": {
        "face": "MesloLGL NF",
        "size": 8
      }
    },
    "list": [
      {
